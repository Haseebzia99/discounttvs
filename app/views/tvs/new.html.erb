<%= simple_form_for (@tv_new) do |f| %>
<div>
  <%= f.input :name %>
  <%= f.input :description %>
  <%= f.input :year %>
  <%= f.input :size %>
  <%= f.input :photos, input_html: {multiple: true}, as: :file, :label => "Upload photos",class: "btn btn-primary" %>
  <%= f.button :submit, class: "btn-main" %>
</div>
<% end %>
